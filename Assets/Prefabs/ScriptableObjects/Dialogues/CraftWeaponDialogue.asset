  dialogueLines:
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u041F\u0440\u0438\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E \u0442\u0435\u0431\u044F
      \u043D\u0430 \u043F\u043E\u043B\u0435 \u0431\u043E\u044F, \u0441\u043E\u043B\u0434\u0430\u0442!"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u041D\u0430 \u043D\u0430\u0448\u0443 \u0431\u0430\u0437\u0443 \u043D\u0430\u043F\u0430\u0434\u0430\u044E\u0442
      \u0432\u0440\u0430\u0433\u0438, \u0438 \u043D\u0430\u043C \u043D\u0443\u0436\u043D\u0430
      \u0442\u0432\u043E\u044F \u043F\u043E\u043C\u043E\u0449\u044C!"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u0414\u0430\u0432\u0430\u0439 \u044F \u043E\u0431\u0443\u0447\u0443 \u0442\u0435\u0431\u044F
      \u043E\u0441\u043D\u043E\u0432\u0430\u043C!"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u0422\u044B \u0442\u043E\u043B\u044C\u043A\u043E \u0440\u043E\u0434\u0438\u043B\u0441\u044F,
      \u043D\u043E \u0443\u0436\u0435 \u0434\u043E\u043B\u0436\u0435\u043D \u0443\u043C\u0435\u0442\u044C
      \u0434\u0432\u0438\u0433\u0430\u0442\u044C\u0441\u044F (WASD), \u0431\u0440\u0430\u0442\u044C
      \u043E\u0440\u0443\u0436\u0438\u0435 \u0432 \u0440\u0443\u043A\u0438 \u0438
      \u0441\u0442\u0440\u0435\u043B\u044F\u0442\u044C!"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u0412\u0438\u0434\u0438\u0448\u044C \u043A\u043E\u0440\u043E\u0431\u043A\u0438
      \u0441 \u043C\u0430\u0442\u0435\u0440\u0438\u0430\u043B\u043E\u043C \u0434\u043B\u044F
      \u043A\u0440\u0430\u0444\u0442\u0430, \u043F\u043E\u044F\u0432\u043B\u044F\u044E\u0449\u0438\u0435\u0441\u044F
      \u0432 \u0441\u0438\u043D\u0435\u0439 \u0437\u043E\u043D\u0435?"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u041F\u043E\u0434\u043E\u0439\u0434\u0438 \u043A \u043B\u044E\u0431\u043E\u043C\u0443
      \u044F\u0449\u0438\u043A\u0443, \u043F\u043E\u0434\u0431\u0435\u0440\u0438
      \u0435\u0433\u043E (E \u0440\u044F\u0434\u043E\u043C \u0441 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u043E\u043C)
      \u0438 \u043E\u0442\u043D\u0435\u0441\u0438 \u0435\u0433\u043E \u043D\u0430
      \u0441\u0442\u0430\u043D\u0446\u0438\u044E \u0438\u0437\u0433\u043E\u0442\u043E\u0432\u043B\u0435\u043D\u0438\u044F
      (\u0437\u0435\u043B\u0451\u043D\u0430\u044F \u043E\u0431\u043B\u0430\u0441\u0442\u044C)"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u0412\u0441\u0435\u0433\u043E \u0442\u044B \u043C\u043E\u0436\u0435\u0448\u044C
      \u0441\u043E\u0437\u0434\u0430\u0432\u0430\u0442\u044C 4 \u0432\u0438\u0434\u0430
      \u043E\u0440\u0443\u0436\u0438\u044F, \u043D\u043E\u0432\u044B\u0445 \u0446\u044B\u043F\u043B\u044F\u0442
      \u0438 \u0442\u0443\u0440\u0435\u043B\u044C!"
    voiceText: {fileID: 0}
  - speakerData: {fileID: 11400000, guid: f12dcc7c4cbd0da4bb37c6f53e67f89b, type: 2}
    text: "\u0410 \u0442\u0435\u043F\u0435\u0440\u044C \u0441\u043E\u0437\u0434\u0430\u0439
      \u043B\u044E\u0431\u043E\u0435 \u043E\u0440\u0443\u0436\u0438\u0435, \u0438
      \u0437\u0430\u043F\u043E\u043C\u043D\u0438 \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E
      \u0431\u043E\u0435\u043F\u0440\u0438\u043F\u0430\u0441\u043E\u0432 \u043E\u0433\u0440\u0430\u043D\u0438\u0447\u0435\u043D\u043E!"
    voiceText: {fileID: 0}
